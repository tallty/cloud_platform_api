<p id="notice"><%= notice %></p>

<h1>Interface Documents</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Site</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interface_documents.each do |interface_document| %>
      <tr>
        <td><%= interface_document.title %></td>
        <td><%= interface_document.description %></td>
        <td><%= interface_document.site %></td>
        <td><%= link_to 'Show', interface_document %></td>
        <td><%= link_to 'Edit', edit_interface_document_path(interface_document) %></td>
        <td><%= link_to 'Destroy', interface_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interface Document', new_interface_document_path %>
